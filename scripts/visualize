#!/usr/bin/env bash

cd modules/visualizeMOT

#clean up past runs
rm -f data
rm -f tracks
rm -f log.txt
rm -f seqs.csv

#link to data
ln -s ../preprocess/out data
ln -s ../sort/output tracks

echo $(date) "Making a list of sequences"
./make_seq_list.py >> log.txt

echo $(date) "Display"
./disp.py seqs.csv

cd ../..


