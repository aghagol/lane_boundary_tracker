{
  "preprocess":
  {
    "pixel_size": 1, //in meters
    "object_size": 10, //for bounding box size
    "scanline_step":1, //subsample scanlines
    "recall": 0.5, //drop detections randomly
    "remove_adjacent_points": false,
        "min_pairwise_dist": 1,
        "search_window_size": 100,
    "split_on_temporal_gaps": true,
        "gap_min": 1, //temporal gaps more than this get split
        "min_seq_size": 2,
    "motion_observations": true, //replace (w,h) with speed (u,v)
    "fake_timestamp": true,
    "use_pose_altitude": true,
        "pose_altitude_offset": 0, //meters (height of vehicle)
    "tag_max_dist": 50, //in meters
    "generate_gt": true
  },
  "sort":
  {
    "d2t_dist_threshold_tight": 1,
    "d2t_dist_threshold_loose": 1,
    "max_age_after_last_update": 20,
    "motion_model_variance": 1,
    "observation_variance": 1,
    "motion_init_pose": true, //this has priority over "motion_init_sync"
    "motion_init_sync": false, //use existing trackers to estimate initial motion
    "constant_fps": false, //KF with fixed time steps
    "bypass_tracking_use_gt": false //bypass tracking and output GT data
  },
  "visualize":
  {
    "delay": 0.01,
    "window_size": 50,
    "centered": true,
    "show_tracks": true,
    "min_track_length": 1,
    "hide_predictions": false,
    "real_time": false,
        "delay_multiplier": 1,
        "max_delay": 5
  },
  "postprocess":
  {
    "stitch": false,
        "gating_thresh": 1,
        "affinity_thresh_min": 2,
        "affinity_thresh_max": 500
  },
  "fuse":
  {
    "enable": true
  }
}
