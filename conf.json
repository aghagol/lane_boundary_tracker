{
  "preprocess":
  {
    "pixel_size": 1, //in meters
    "object_size": 10, //for bounding box size
    "confidence_thresh": 0.5, //threshold for CNN output
    "recall": 1, //drop detections randomly
    "generate_gt": true,
    "remove_adjacent_points": false,
        "min_pairwise_dist": 1,
        "search_window_size": 100,
    "split_on_temporal_gaps": true,
        "gap_min": 1, //temporal gaps more than this get split
        "min_seq_size": 2,
    "motion_observations": true, //replace (w,h) with speed (u,v)
    "enable_tagging": true,
        "tag_max_dist": 10, //in meters
        "pose_filter_bbox": true,
        "fake_timestamp": true,
        "tag_altitude": true,
            "pose_altitude_offset": 0 //meters (height of vehicle)
  },
  "sort":
  {
    "bypass_tracking_use_gt": false, //bypass tracking and output GT data
    "d2t_dist_threshold_tight": 1,
    "d2t_dist_threshold_loose": 1,
    "max_age_after_last_update": 10, //minimum age to kill the track
    "max_mov_after_last_update": 5, //minimum gap to kill the track
    "motion_model_variance": 0.01,
    "observation_variance": 100,
    "motion_init_pose": true, //initialize motion with pose which has priority over "motion_init_sync"
    "motion_init_sync": false //initialize motion with existing trackers
  },
  "visualize":
  {
    "delay": 0.01,
    "window_size": 50,
    "centered": true,
    "show_tracks": true,
    "min_track_length": 1,
    "hide_predictions": true,
    "real_time": false,
        "delay_multiplier": 1,
        "max_delay": 5
  },
  "postprocess":
  {
    "stitch": false,
        "gating_thresh": 1,
        "affinity_thresh_min": 2,
        "affinity_thresh_max": 500
  },
  "fuse":
  {
    "enable": true,
    "min_seq_length": 1
  }
}
