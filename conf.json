{
  "preprocess":
  {
    "pixel_size": 1,
    "object_size": 10,
    "reduce_short_ransac_lines": true,
        "min_line_length": 2,
    "remove_adjacent_endpoints": true,
        "min_pairwise_dist": 3,
        "search_window_size": 50,
    "remove_chunk_overflow": true,
    "rank_reduction": false,
    "video_tracking": false,
        "start_with_pose": true,
            "head_start": 10,
            "pose_step": 1,
        "fake_dets": true,
            "fake_dets_n": 50,
            "fake_dets_pose_step": 1,
            "fake_dets_min_dist": 0.3,
        "smooth_pose": false
  },
  "sort":
  {
    "d2t_dist_threshold_tight": 2,
    "d2t_dist_threshold_loose": 3,
    "max_age_after_last_update": 30,
    "max_age_with_no_updates": 10,
    "motion_model_variance": 0.00000001,
    "observation_variance": 1000,
    "sync_initialization": true
  },
  "visualize":
  {
    "delay": 0.01,
    "window_size": 200,
    "show_tracks": true,
    "min_track_length": 1,
    "hide_predictions": false
  },
  "postprocess":
  {
    "stitch": false,
        "gating_thresh": 1,
        "affinity_thresh_min": 1,
        "affinity_thresh_max": 500
  },
  "fuse":
  {
    "enable": true
  }
}
