{
  "preprocess":
  {
    "pixel_size": 1,
    "object_size": 10,
    "remove_adjacent_points": false,
        "min_pairwise_dist": 1,
        "search_window_size": 100,
    "rank_reduction": false,
    "video_tracking": false,
        "start_with_pose": true,
            "head_start": 10,
            "pose_step": 1,
        "fake_dets": true,
            "fake_dets_n": 50,
            "fake_dets_pose_step": 1,
            "fake_dets_min_dist": 0.3,
        "smooth_pose": false,
    "split_on_temporal_gaps": true,
        "gap_min": 3, //temporal gaps more than this get split
        "min_seq_size": 20
  },
  "sort":
  {
    "d2t_dist_threshold_tight": 1,
    "d2t_dist_threshold_loose": 1,
    "max_age_after_last_update": 50,
    "motion_model_variance": 1,
    "observation_variance": 1,
    "sync_initialization": false,
    "real_time": true
  },
  "visualize":
  {
    "delay": 0.1,
    "window_size": 30,
    "centered": true,
    "show_tracks": true,
    "min_track_length": 1,
    "hide_predictions": true,
    "real_time": true,
        "delay_multiplier": 0.1,
        "max_delay": 1
  },
  "postprocess":
  {
    "stitch": false,
        "gating_thresh": 1,
        "affinity_thresh_min": 2,
        "affinity_thresh_max": 500
  },
  "fuse":
  {
    "enable": false
  }
}
