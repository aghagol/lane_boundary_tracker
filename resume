  --tracker "sort" \
  --config  ./conf.json \
  --verbosity 1 \
  --visualize 0 \
  --fuses   ~/data_ian/input/fuse/ \
  --tagged  ~/data_ian/input/fuse_tagged/ \
  --cache   $CACHE
