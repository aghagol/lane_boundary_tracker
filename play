#!/usr/bin/env bash

CACHE="/home/mo/Desktop/tmp/"

python line_connector.py \
  --images  ~/data_ian/input/images/ \
  --poses   ~/data_ian/input/poses/ \
  --chunks  ~/data_ian/input/chunks/ \
  --drives  ~/Desktop/drive_poc.txt \
  --config  ./conf.json \
  --verbosity 1 \
  --visualize 2 \
  --fuses   $CACHE/input/fuse/ \
  --tagged  $CACHE/input/fuse_tagged/ \
  --cache   $CACHE \
  --clear_cache
